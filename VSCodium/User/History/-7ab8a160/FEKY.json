{
    "editor.fontSize": 16,
    "editor.fontFamily": "'Ubuntu Mono'",
    "workbench.colorTheme": "Monospace IDX Dark",
    "workbench.iconTheme": "material-icon-theme",
    "editor.tokenColorCustomizations": {"textMateRules": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#969896",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#198844"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#a36ac7"
      }
    },
    {
      "scope": ["keyword", "storage", "keyword.operator"],
      "settings": {
        "foreground": "#a36ac7"
      }
    },
    {
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#3971ed"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#fba922"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#3971ed"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#cc342b"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#fba922"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#cc342b"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#3971ed"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#3971ed"
      }
    }
  ]}
}